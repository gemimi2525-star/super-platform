# Authentication (Dev Harness)
# Set to 'true' to enable Dev Bypass Mode (requires x-dev-test- headers)
AUTH_DEV_BYPASS=false
NODE_ENV=development
ENABLE_TEST_LOGIN=true

# Firebase Configuration (Service Account)
FIREBASE_PROJECT_ID=
